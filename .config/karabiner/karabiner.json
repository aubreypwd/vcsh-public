{
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "F3 → Show Notification Center / Shift + F3 → Mission Control",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.hegenberg\\.KeyboardCleanTool$"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": {
                                    "key_code": "f3",
                                    "modifiers": {
                                        "mandatory": ["shift"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "apple_vendor_keyboard_key_code": "mission_control" }],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.hegenberg\\.KeyboardCleanTool$"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": {
                                    "key_code": "f3",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "right_arrow",
                                        "modifiers": ["left_command", "left_shift", "left_control", "left_option"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
