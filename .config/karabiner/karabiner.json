                            {
                                "from": {
                                    "key_code": "f5",
                                    "modifiers": {
                                        "mandatory": ["shift"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "t",
                                        "modifiers": ["left_control", "left_option", "left_command", "left_shift"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "devices": [
                {
                    "identifiers": { "is_keyboard": true },
